<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.gmaking.pve.dao.MapDAO">
    <resultMap id="MapResultMap" type="com.project.gmaking.pve.vo.MapVO">
        <id column="MAP_ID" property="mapId"/>
        <result column="MAP_NAME" property="name"/>
        <result column="CREATED_DATE" property="createdDate"/>
        <result column="CREATED_BY" property="createdBy"/>
        <result column="UPDATED_DATE" property="updatedDate"/>
        <result column="UPDATED_BY" property="updatedBy"/>
    </resultMap>

    <select id="selectAllMaps" resultMap="MapResultMap">
        SELECT *
        FROM TB_MAP
        ORDER BY MAP_ID ASC
    </select>
</mapper>
